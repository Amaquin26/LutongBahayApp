            <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="m1 1 6 6m0 0 6 6M7 7l6-6M7 7l-6 6" />
        </svg>
    </button>
</div>


<script>
    function incrementQty(foodId,quantity,price){
        let inputVal = document.getElementById(`product-${foodId}`);
        let curVal = parseInt(quantity) + 1;
        inputVal.value = curVal

        fetch('/Basket/QuantityChange', {
            method: 'PUT',
            headers: {
                'Content-Type': 'application/json'
            },
            body: JSON.stringify({ foodId, quantity: curVal })
        })
        .then(data => {
            checkFood(foodId, curVal, price)
        })
        .catch(error => {
            document.getElementById('toast-danger-message').innerHTML = 'Operation unsuccessful';
            showDangerToast();
            inputVal.value = parseInt(quantity);
        });

    }

    function decrementQty(foodId, quantity, price) {
        if (quantity <= 1) return;

        let inputVal = document.getElementById(`product-${foodId}`);
        let curVal = parseInt(quantity) - 1;
        inputVal.value = curVal

        fetch('/Basket/QuantityChange', {
            method: 'PUT',
            headers: {
                'Content-Type': 'application/json'
            },
            body: JSON.stringify({ foodId, quantity: curVal })
        })
        .then(data => {
                checkFood(foodId, curVal, price)
        })
        .catch(error => {
            document.getElementById('toast-danger-message').innerHTML = 'Operation unsuccessful';
            showDangerToast();
            inputVal.value = parseInt(quantity);
        });
    }

    function checkFood(foodId,quantity,price) {
        const checkStatus = document.getElementById(`checkbox-table-${foodId}`).checked || false;
        const totalSelected = document.getElementById('total-selected');
        const amountDue = document.getElementById('amount-due');
        const checkoutButton = document.getElementById('checkout-button');
        quantity = parseInt(quantity);
        const fb = { foodId, quantity, price };
        
        const existingItemIndex = foodBaskets.findIndex(item => item.foodId === foodId);

        if (existingItemIndex === -1) {
            // Item not found, add it to the basket
            if (checkStatus)
                foodBaskets.push(fb);
        } else {
            if (checkStatus) {
                // Item found and checked, update quantity
                foodBaskets[existingItemIndex].quantity = quantity;
            } else {
                // Item found but unchecked, remove it
                foodBaskets.splice(existingItemIndex, 1);
            }
        }
        sum = 0;
        foodBaskets.forEach(item => {
            sum += item.quantity * item.price;
        });
        //console.log(foodBaskets);
        totalSelected.innerHTML = foodBaskets.length
        amountDue.innerHTML = sum;

        const addressContainer = document.getElementById('address-container');
        const paymentContainer = document.getElementById('payment-container');
        const paymentLabel = document.getElementById('payment-label');

        if (foodBaskets.length > 0) {
            checkoutButton.disabled = false;

            addressContainer.classList.remove('hidden');
            addressContainer.classList.add('block');

            paymentContainer.classList.remove('hidden');
            paymentContainer.classList.add('block');

            paymentLabel.classList.remove('hidden');
            paymentLabel.classList.add('block');
        }else{
            checkoutButton.disabled = true;

            addressContainer.classList.remove('block');
            addressContainer.classList.add('hidden');

            paymentContainer.classList.remove('block');
            paymentContainer.classList.add('hidden');

            paymentLabel.classList.remove('block');
            paymentLabel.classList.add('hidden');
        }

        checkoutButton.disabled = foodBaskets.length > 0 ? false : true;
    }

    function proceedCheckOut() {   
        const orderAddress = document.getElementById('order-add').value || null;

        const checkout = {
            foodBaskets: foodBaskets,
            totalDue:sum,
            orderAddress: orderAddress
        }

        fetch('/Order/CheckOutFoods', {
            method: 'POST',
            headers: {
                'Content-Type': 'application/json'
            },
            body: JSON.stringify(checkout)
        })
        .then(data => {
            window.location.href = "/Order/"
        })
        .catch(error => {
            document.getElementById('toast-danger-message').innerHTML = "Can't proceed to Checkout";
            showDangerToast();
            inputVal.value = parseInt(quantity);
        });
    }

    function showDangerToast() {
        const toastDanger = document.getElementById('toast-danger');
        toastDanger.classList.remove('hidden');
        toastDanger.classList.add('sticky');
    }

    function selectPaymentOption(option){
        option = parseInt(option);
        const cod = document.getElementById('cod');
        const mastercard = document.getElementById('mastercard');
        const visa = document.getElementById('visa');
        const gcash = document.getElementById('gcash');

        switch (option) {
            case 1:
                cod.classList.remove('border-primary');
                cod.classList.add('border-primary');

                mastercard.classList.remove('border-primary');
                visa.classList.remove('border-primary');
                gcash.classList.remove('border-primary');
                break;
            case 2:
                mastercard.classList.remove('border-primary');
                mastercard.classList.add('border-primary');

                cod.classList.remove('border-primary');
                visa.classList.remove('border-primary');
                gcash.classList.remove('border-primary');
                break;
            case 3:
                visa.classList.remove('border-primary');
                visa.classList.add('border-primary');

                cod.classList.remove('border-primary');
                mastercard.classList.remove('border-primary');
                gcash.classList.remove('border-primary');
                break;
            case 4:
                gcash.classList.remove('border-primary');
                gcash.classList.add('border-primary');

                cod.classList.remove('border-primary');
                mastercard.classList.remove('border-primary');
                visa.classList.remove('border-primary');
                break;
            default:
                cod.classList.remove('border-primary');
                cod.classList.add('border-primary');

                mastercard.classList.remove('border-primary');
                visa.classList.remove('border-primary');
                gcash.classList.remove('border-primary');
        }
    }

    document.getElementById('hide-danger-toast').addEventListener('click', () => {
        const toast = document.getElementById('toast-danger');
        toast.classList.remove('sticky');
        toast.classList.add('hidden');
    })
</script>